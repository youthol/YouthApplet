<view class="page">
  <view class="weui-cells__title">已绑定的信息</view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd">姓名</view>
      <view class="weui-cell__ft">{{userInfo.name}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">学工号</view>
      <view class="weui-cell__ft">{{userInfo.sdutid}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">学院</view>
      <view class="weui-cell__ft">{{userInfo.college}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">班级</view>
      <view class="weui-cell__ft">{{userInfo.grade}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">宿舍楼</view>
      <view class="weui-cell__ft">{{userInfo.dormitory}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">房间号</view>
      <view class="weui-cell__ft">{{userInfo.room}}</view>
    </view>
    <view class="weui-cell">
      <view class="weui-cell__bd">手机号</view>
      <view class="weui-cell__ft">{{userInfo.phone}}</view>
    </view>
  </view>
  <view class="weui-btn-area">
    <button type="warn" bindtap="unBind">解除绑定</button>
  </view>
</view>